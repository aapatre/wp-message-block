{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "message-block/message-block",
	"version": "0.1.0",
	"title": "WP Message Block",
	"category": "text",
	"icon": "dismiss",
	"description": "A closeable message block to show your visitors.",
	"supports": {
		"html": false
	},
	"textdomain": "message-block",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"attributes": {
		"message": {
			"type": "string",
			"source": "html",
			"selector": "p"
		}
	}
}